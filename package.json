{
  "name": "coursework",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "element-react": "^1.4.1",
    "element-theme-default": "^1.4.10",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-scripts": "1.0.17",
    "react-syntax-highlighter": "^6.1.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css":
      "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "cross-env NODE_PATH=src react-scripts start & yarn watch-css",
    "build": "react-scripts build & yarn build-css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "axios": "^0.17.1",
    "cross-env": "^5.1.1",
    "highlight.js": "^9.12.0",
    "immutable": "^3.8.2",
    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "node-sass": "^4.7.1",
    "node-sass-chokidar": "^0.0.3",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "react-cookie": "^2.1.2",
    "react-document-title": "^2.0.3",
    "react-highlight": "^0.10.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6"
  }
}
